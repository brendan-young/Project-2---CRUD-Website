<%- include('./partials/head.ejs') %>

<h1> Property: <%= rental.address %></h1>

<img class="show-img" src="<%= rental.imageURL %>" alt="" />

<h2>Real Estate: 
    <a href="<%= rental.linkURL %>" ><%= rental.realEstate %></a>
</h2>

<p>
    Rooms: <%= rental.rooms %>
    <br>
    Bathrooms: <%= rental.bathrooms %>
    <br>
    Extras: <br>
     <%= rental.extras %>
    <br>
    <% if (rental.petFriendly) { %>
        This property is pet friendly.
        <% } else { %>
            This property is not pet friendly.
        <% } %>
    <br>
    Rental Price: <%= rental.rentPrice %>
    <br>
    Additional Information: <br>
    <%= rental.finalComments %>
    <br>

</p>


<%- include('./partials/footer.ejs') %>

